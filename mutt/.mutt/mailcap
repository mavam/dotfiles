text/plain;     less -S

text/html;      qlmanage -p %s > /dev/null 2>&1; nametemplate=%s.html
text/html;      w3m -v -F -T text/html %s; nametemplate=%s.html; copiousoutput

image/*;        qlmanage -p %s > /dev/null 2>&1
audio/*;        qlmanage -p %s > /dev/null 2>&1
video/*;        qlmanage -p %s > /dev/null 2>&1

application/*;  ~/.mutt/scripts/view %s
